{
  "name": "ts-react-ssr-basis",
  "version": "1.0.0",
  "license": "MIT",
  "repository": "github:Dattaya/ts-react-ssr-basis",
  "description": "React, TypeScript, webpack unopinionated (mostly) boilerplate",
  "author": "Yaroslav Kiliba",
  "scripts": {
    "start": "concurrently --raw \"npm run build:server-dev\" \"npm run build:client-dev\"",
    "start:prod": "node server-prod-dist",
    "build:start": "npm run build && npm run start:prod",
    "build": "concurrently \"npm run build:server-prod\" \"npm run build:client-prod\"",
    "build:client-dev": "cross-env BABEL_ENV=client-dev webpack serve --progress --config webpack/client-dev.js",
    "build:client-prod": "cross-env BABEL_ENV=client-prod NODE_ENV=production webpack --config webpack/client-prod.js",
    "build:server-dev": "cross-env BABEL_ENV=server webpack --progress --config webpack/server-dev.js",
    "build:server-prod": "cross-env BABEL_ENV=server NODE_ENV=production webpack --config webpack/server-prod.js",
    "tsc": "tsc",
    "lint": "eslint --ext .ts,.tsx client/ common/ server/",
    "lint:staged": "lint-staged",
    "test": "cross-env BABEL_ENV=test jest --coverage --config jest/jest.config.js"
  },
  "browserslist": [
    ">0.5%",
    "last 2 versions",
    "last 4 safari versions",
    "Firefox ESR",
    "not dead",
    "not IE 11"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@loadable/component": "^5.15.0",
    "@loadable/server": "^5.15.1",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-constant-elements": "^7.14.5",
    "@babel/plugin-transform-react-inline-elements": "^7.14.5",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/register": "^7.15.3",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/webpack-plugin": "^5.15.1",
    "@types/css-modules": "^1.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/loadable__component": "^5.13.4",
    "@types/loadable__server": "^5.12.6",
    "@types/node": "^14.0.0",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-test-renderer": "^17.0.1",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "acorn": "^8.4.1",
    "autoprefixer": "^10.3.3",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "clean-webpack-plugin": "4.0.0-alpha.0",
    "concurrently": "^6.2.1",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "eslint": "^7.32.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^7.0.1",
    "jest": "^27.1.0",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^2.2.0",
    "postcss": "^8.3.6",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "react-test-renderer": "^17.0.2",
    "style-loader": "^3.2.1",
    "typescript": "^4.4.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0",
    "webpack-shell-plugin-next": "2.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{tsx,ts}": [
      "eslint --max-warnings=0 --fix",
      "git add --force"
    ]
  }
}
