{
  "name": "ts-react-ssr-basis",
  "version": "1.0.0",
  "license": "MIT",
  "repository": "github:Dattaya/ts-react-ssr-basis",
  "description": "React, TypeScript, webpack unopinionated (mostly) boilerplate",
  "author": "Yaroslav Kiliba",
  "scripts": {
    "start": "concurrently \"npm run build:server-dev\" \"npm run build:client-dev\"",
    "start:prod": "node server-prod-dist",
    "build:start": "npm run build && npm run start:prod",
    "build": "concurrently \"npm run build:server-prod\" \"npm run build:client-prod\"",
    "build:client-dev": "cross-env BABEL_ENV=client-dev webpack-dev-server --config webpack/client-dev.js",
    "build:client-prod": "cross-env BABEL_ENV=client-prod NODE_ENV=production webpack --config webpack/client-prod.js",
    "build:server-dev": "cross-env BABEL_ENV=server webpack --config webpack/server-dev.js",
    "build:server-prod": "cross-env BABEL_ENV=server NODE_ENV=production webpack --config webpack/server-prod.js",
    "tsc": "tsc",
    "lint": "eslint --ext .ts,.tsx client/ common/ server/",
    "test": "cross-env BABEL_ENV=test jest --coverage --config jest/jest.config.js"
  },
  "browserslist": [
    ">0.2%",
    "not dead"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "@loadable/component": "^5.12.0",
    "@loadable/server": "^5.12.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^7.0.0",
    "express": "^4.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet-async": "^1.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-constant-elements": "^7.8.3",
    "@babel/plugin-transform-react-inline-elements": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@loadable/babel-plugin": "^5.12.0",
    "@loadable/webpack-plugin": "^5.12.0",
    "@types/css-modules": "^1.0.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.3",
    "@types/loadable__component": "^5.10.0",
    "@types/loadable__server": "^5.9.1",
    "@types/node": "^10.17.16",
    "@types/react": "^16.9.21",
    "@types/react-dom": "^16.9.5",
    "@types/react-test-renderer": "^16.9.2",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "css-loader": "^3.4.2",
    "cssnano-preset-advanced": "^4.0.7",
    "eslint": "^6.8.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.4.0",
    "file-loader": "^5.1.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^25.1.0",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "react-test-renderer": "^16.12.0",
    "style-loader": "^1.1.3",
    "typescript": "^3.8.2",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-node-externals": "^1.7.2",
    "webpack-shell-plugin-next": "0.6.4",
    "webpackbar": "^4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint -- --fix",
      "git add"
    ]
  }
}
